{
    "name": "in-bundle-insights-panel",
    "version": "1.0.0",
    "description": "Panel which displays the list of bundles which contain the selected item",
    "author": "RWS",
    "homepage": "https://www.rws.com",
    "license": "Apache-2.0",
    "type": "module",
    "main": "../dist/in-bundle-insights-panel/main.js",
    "files": [
        "../dist/in-bundle-insights-panel/*"
    ],
    "private": true,
    "scripts": {
        "build": "webpack --config ./webpack.prod.config.js --progress",
        "dev": "webpack serve --config ./webpack.dev.config.js --progress --env target=http://0.0.0.0:8080 manifest=../manifest.json",
        "pack": "sites-extensions pack --manifest=../manifest.json --input=../dist --output=../",
        "prepack": "npm run build",
        "lint": "eslint . --fix && prettier --write \"src/**/*\""
    },
    "peerDependencies": {
        "@tridion-sites/extensions": "3.0.0",
